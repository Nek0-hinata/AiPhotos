.page {
  height: 750rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.page .camera {
  padding: 20px;
  border-radius: 50%;
  border-width: 1px;
  border-style: solid;
}
.page .text {
  margin: 30px 0;
  font-size: 18px;
  letter-spacing: 2px;
}
.page .btn {
  position: absolute;
  bottom: 100rpx;
}