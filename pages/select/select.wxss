.page {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
}
.page .part {
  width: 500rpx;
  padding: 40rpx 0;
  margin: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 10px;
  border-style: solid;
  border-width: 1px;
}